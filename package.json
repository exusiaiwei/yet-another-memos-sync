{
	"name": "yet-another-memos-sync",
	  "version": "1.4.1",
	"description": "Enhanced Obsidian plugin for syncing Memos with emoji timeline, List Callout format support and improved deletion detection",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"release-notes": "node generate-release-notes.js"
	},
	"keywords": [
		"obsidian",
		"memos",
		"sync",
		"daily-notes",
		"emoji",
		"timeline"
	],
	"author": "exusiaiwei",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"obsidian-daily-notes-interface": "^0.9.4",
		"moment": "^2.29.4"
	}
}
